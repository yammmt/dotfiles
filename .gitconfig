[user]
	name = yammmt
	email = cu.cs.y20@gmail.com
[color]
	diff = auto
	status = auto
	branch = auto
	ui = auto
[core]
	precomposeunicode = true
	editor = nano
	autocrlf = false
	quotepath = false
[push]
	default = matching
[merge]
	ff = false
[pull]
	ff = only
[alias]
	br = branch
	st = status
	co = checkout
	gr = log --graph --decorate
	b = branch
	cm = commit
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
