[user]
	name = yammmt
	email = cu.cs.y20@gmail.com
	signingkey = B4EE4EA3
[color]
	diff = auto
	status = auto
	branch = auto
	ui = auto
[core]
	precomposeunicode = true
	editor = nano
	autocrlf = false
	quotepath = false
	excludesfile = /Users/masayama/.gitignore_global
[push]
	default = matching
[merge]
	ff = false
[pull]
	ff = only
[alias]
	br = branch
	st = status
	co = checkout
	gr = log --graph --decorate
	b = branch
	cm = commit
	last = log --graph --decorate -p -1
	pp = pull --prune
	d = diff
	a = add
	ol = log --oneline
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[gpg]
	program = gpg2
[commit]
	gpgsign = true
